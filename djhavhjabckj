#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int a=1,b=2;;
	if(n>0 and n<100){
     for(int i=1;i<=n;++i){
        for(int j=1;j<=1;++j){
            cout<<a<<'\t';
            ++a;
        }
        if(i>=3){
            for(int j=1;j<=(i-2);++j){
                cout<<"0"<<'\t';
            }
        }
        if(i>=2){
            for(int j=1;j<=1;++j){
                cout<<b<<'\t';
                ++b;
            }

        }
        cout<<endl;
     }
    }
	else{
		cout<<"wrong Input Entered";
	}
    return 0;
}    
